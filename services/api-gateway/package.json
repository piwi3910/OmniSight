    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "socket.io": "^4.6.1"
